    "concurrently": "^4.1.0",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "if-env": "^1.0.4",
    "inquirer": "^7.0.0",
    "mysql": "^2.17.1",
    "node": "^12.10.0"
  }
}
